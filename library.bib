file = {:C$\backslash$:/Users/Grzegorz Brzezinka/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jermendy - 2012 - Vascular memory can we broaden the concept of the metabolic memory.pdf:pdf},
