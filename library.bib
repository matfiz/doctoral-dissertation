issn = {1099-1352},
journal = {Journal of molecular recognition : JMR},
keywords = {actinomycin d,apoptosis,atomic force microscopy,bcl-2,cell stiffness,nanoindentation spectroscopy,rho,trail},
month = may,
number = {5},
pages = {299--308},
pmid = {22528192},
title = {{Stiffness changes of tumor HEp2 cells correlates with the inhibition and release of TRAIL-induced apoptosis pathways.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22528192},
volume = {25},
year = {2012}
