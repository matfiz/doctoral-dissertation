journal = {Integrative biology : quantitative biosciences from nano to macro},
month = mar,
number = {3},
pages = {300--11},
pmid = {24435061},
title = {{Global architecture of the F-actin cytoskeleton regulates cell shape-dependent endothelial mechanotransduction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24435061},
volume = {6},
year = {2014}
}
