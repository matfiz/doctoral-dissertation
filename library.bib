journal = {Journal of electron microscopy},
keywords = {Actins,Actins: ultrastructure,Animals,Astrocytes,Astrocytes: physiology,Astrocytes: ultrastructure,Cell Membrane,Cell Membrane: ultrastructure,Cerebral Cortex,Cerebral Cortex: cytology,Elasticity,Glutaral,Microscopy, Atomic Force,Rats,Rats, Wistar,Tissue Fixation},
month = jan,
number = {3},
pages = {463--71},
