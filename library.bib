journal = {PloS one},
keywords = {Algorithms,Animals,Automatic Data Processing,Automatic Data Processing: methods,Automation,Biology,Biology: methods,Biophysics,Biophysics: methods,Equipment Design,Escherichia coli,Escherichia coli: genetics,Escherichia coli: metabolism,Humans,Micromanipulation,Micromanipulation: methods,Microscopy, Atomic Force,Microscopy, Atomic Force: methods,Models, Statistical,Poisson Distribution,Static Electricity},
month = jan,
number = {4},
pages = {e18887},
pmid = {21559483},
