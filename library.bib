issn = {0304-3991},
journal = {Ultramicroscopy},
keywords = {Animals,Artifacts,Cells, Cultured,Cricetinae,Fixatives,Kidney,Kidney: ultrastructure,Microscopy, Atomic Force,Microscopy, Atomic Force: methods,Tissue Fixation,Tissue Fixation: methods},
