issn = {1432-2013},
journal = {Pfl\"{u}gers Archiv : European journal of physiology},
keywords = {HG,endothelial cells,hyperglycaemia,mitochondrial respiration,oxidative,uncoupling protein},
mendeley-tags = {HG},
month = dec,
