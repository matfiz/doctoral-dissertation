journal = {Analytical and bioanalytical chemistry},
keywords = {Animals,Cells, Cultured,Cytoplasm,Cytoplasm: metabolism,Electron Spin Resonance Spectroscopy,Humans,Mice,Microscopy, Confocal,Mitochondria,Mitochondria: metabolism,NIH 3T3 Cells,Reactive Oxygen Species,Reactive Oxygen Species: analysis,Reactive Oxygen Species: metabolism,Signal Transduction,Stress, Physiological},
month = jun,
number = {8},
pages = {2383--90},
