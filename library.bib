issn = {1542-0086},
journal = {Biophysical journal},
keywords = {Animals,Cell Membrane,Cell Membrane: physiology,Cells, Cultured,Computer Simulation,Elasticity,Hippocampus,Hippocampus: physiology,Membrane Microdomains,Membrane Microdomains: physiology,Models, Neurological,Nanostructures,Neurons,Neurons: physiology,Rats,Stress, Mechanical},
month = feb,
number = {4},
pages = {1521--32},
pmid = {17981897},
title = {{Elastic membrane heterogeneity of living cells revealed by stiff nanoscale membrane domains.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2212673\&tool=pmcentrez\&rendertype=abstract},
