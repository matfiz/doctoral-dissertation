title = {{The influence of high glucose on the aerobic metabolism of endothelial EA.hy926 cells.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3513600\&tool=pmcentrez\&rendertype=abstract},
volume = {464},
