@article{Quinta2011,
abstract = {Cytoskeletal structure is continually remodeled to accommodate normal cell growth and to respond to pathophysiological cues. As a consequence, several cytoskeleton-interacting proteins become involved in a variety of cellular processes such as cell growth and division, cell movement, vesicle transportation, cellular organelle location and function, localization and distribution of membrane receptors, and cell-cell communication. Molecular chaperones and immunophilins are counted among the most important proteins that interact closely with the cytoskeleton network, in particular with microtubules and microtubule-associated factors. In several situations, heat-shock proteins and immunophilins work together as a functionally active heterocomplex, although both types of proteins also show independent actions. In circumstances where homeostasis is affected by environmental stresses or due to genetic alterations, chaperone proteins help to stabilize the system. Molecular chaperones facilitate the assembly, disassembly and/or folding/refolding of cytoskeletal proteins, so they prevent aberrant protein aggregation. Nonetheless, the roles of heat-shock proteins and immunophilins are not only limited to solve abnormal situations, but they also have an active participation during the normal differentiation process of the cell and are key factors for many structural and functional rearrangements during this course of action. Cytoskeleton modifications leading to altered localization of nuclear factors may result in loss- or gain-of-function of such factors, which affects the cell cycle and cell development. Therefore, cytoskeletal components are attractive therapeutic targets, particularly microtubules, to prevent pathological situations such as rapidly dividing tumor cells or to favor the process of cell differentiation in other cases. In this review we will address some classical and novel aspects of key regulatory functions of heat-shock proteins and immunophilins as housekeeping factors of the cytoskeletal network.},
author = {Quint\'{a}, H\'{e}ctor R and Galigniana, Natalia M and Erlejman, Alejandra G and Lagadari, Mariana and Pilipuk, Graciela Piwien and Galigniana, Mario D},
doi = {10.1016/j.cellsig.2011.07.023},
file = {:C$\backslash$:/Users/Grzegorz Brzezinka/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Quint\'{a} et al. - 2011 - Man. - cellular signalling - 2011 - management of cytoskeleton architecture by molecular chaperones and immunophilins: - cellular signalling - 2011 - management of cytoskeleton architecture by molecular chaperones and immunophilins},
issn = {1873-3913},
journal = {Cellular signalling},
keywords = {Folding,Heat-shock protein,Intermediate filament,Microfilament,Microtubule,Stress},
month = aug,
number = {12},
pages = {1907--1920},
pmid = {21864675},
publisher = {Elsevier Inc.},
title = {{Management of cytoskeleton architecture by molecular chaperones and immunophilins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21864675},
volume = {23},
year = {2011}
}
