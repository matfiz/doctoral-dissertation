isbn = {4047129550},
issn = {0021-9258},
journal = {The Journal of biological chemistry},
keywords = {Animals,Aorta,Ascorbic Acid,Ascorbic Acid: chemistry,Ascorbic Acid: pharmacology,Biopterin,Biopterin: analogs \& derivatives,Biopterin: chemistry,Biopterin: pharmacology,Cattle,Cells, Cultured,Endothelium, Vascular,Endothelium, Vascular: enzymology,Kinetics,Nitric Oxide Donors,Nitric Oxide Donors: pharmacology,Nitric Oxide Synthase,Nitric Oxide Synthase Type III,Nitric Oxide Synthase: metabolism,Peroxynitrous Acid,Peroxynitrous Acid: chemistry,Peroxynitrous Acid: pharmacology,Spin Labels,Sulfhydryl Compounds,Sulfhydryl Compounds: chemistry,Sulfhydryl Compounds: pharmacology},
