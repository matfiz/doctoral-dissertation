journal = {Journal of cell science},
keywords = {Cell Adhesion,Cell Adhesion: physiology,Cell Line,Epitopes,Epitopes: immunology,Fibronectins,Fibronectins: metabolism,Glycocalyx,Glycocalyx: physiology,Glycocalyx: ultrastructure,Glycosylation,Humans,Interferon-gamma,Interferon-gamma: metabolism,Microscopy,Microscopy: methods,Receptors, IgG,Receptors, IgG: immunology},
