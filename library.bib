journal = {Free radical biology \& medicine},
keywords = {Animals,Blood Coagulation,Blood Vessels,Blood Vessels: metabolism,Blood Vessels: pathology,Capillary Permeability,Humans,Inflammation,Inflammation: metabolism,Inflammation: pathology,Neovascularization, Pathologic,Neovascularization, Pathologic: metabolism,Reactive Nitrogen Species,Reactive Nitrogen Species: metabolism,Reactive Oxygen Species,Reactive Oxygen Species: metabolism,Thrombosis,Thrombosis: metabolism,Vasomotor System,Vasomotor System: metabolism,Vasomotor System: pathology},
month = feb,
