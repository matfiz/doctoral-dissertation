journal = {Diabetes},
keywords = {3-O-Methylglucose,Abdominal,Abdominal: drug effects,Abdominal: metabolism,Animals,Aorta,Biological Transport,Biological Transport: drug effects,Blotting,Cattle,Cell Membrane,Cell Membrane: metabolism,Cells,Cultured,Deoxyglucose,Deoxyglucose: metabolism,Endothelium,Glucose,Glucose: pharmacology,HG,Kinetics,Messenger,Messenger: metabolism,Methylglucosides,Methylglucosides: metabolism,Monosaccharide Transport Proteins,Monosaccharide Transport Proteins: biosynthesis,Monosaccharide Transport Proteins: metabolism,Monosaccharides,Monosaccharides: pharmacology,Muscle,Northern,RNA,RNA: genetics,RNA: isolation \& purification,Smooth,Tritium,Vascular,Vascular: drug effects,Vascular: metabolism},
