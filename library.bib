issn = {0006-3002},
journal = {Biochimica et biophysica acta},
keywords = {Antioxidants,Antioxidants: pharmacology,Cell Death,Cell Respiration,Darkness,Gramicidin,Gramicidin: metabolism,HeLa Cells,Humans,Hydrogen Peroxide,Hydrogen Peroxide: pharmacology,Light,Lipid Bilayers,Lipid Bilayers: chemistry,Methacrylates,Methacrylates: pharmacology,Mitochondria,Mitochondria: drug effects,Mitochondria: physiology,Mitochondria: radiation effects,Onium Compounds,Onium Compounds: pharmacology,Organophosphorus Compounds,Organophosphorus Compounds: pharmacology,Oxidative Stress,Photochemotherapy,Photosensitizing Agents,Photosensitizing Agents: pharmacology,Reactive Oxygen Species,Reactive Oxygen Species: metabolism,Rotenone,Rotenone: pharmacology,Singlet Oxygen,Singlet Oxygen: metabolism,Superoxides,Superoxides: metabolism,Thiazoles,Thiazoles: pharmacology,Ubiquinone,Ubiquinone: analogs \& derivatives,Ubiquinone: pharmacology},
