doi = {10.1016/j.mvr.2009.04.002},
file = {:D$\backslash$:/Brzezinka/Dokumenty/doktoranckie/literatura/Liu, Yang/Liu, Yang - Electrokinetic effect of the endothelial glycocalyx layer on two-phase blood flow in small blood vessels. - 2009.pdf:pdf},
