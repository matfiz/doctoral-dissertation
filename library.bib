journal = {European journal of pharmacology},
keywords = {Adoptive Transfer,Animals,Anti-Inflammatory Agents,Anti-Inflammatory Agents: pharmacology,Anti-Inflammatory Agents: therapeutic use,Benzofurans,Benzofurans: pharmacology,CD4-Positive T-Lymphocytes,CD4-Positive T-Lymphocytes: drug effects,CD4-Positive T-Lymphocytes: immunology,CD4-Positive T-Lymphocytes: transplantation,CD8-Positive T-Lymphocytes,CD8-Positive T-Lymphocytes: drug effects,CD8-Positive T-Lymphocytes: immunology,CD8-Positive T-Lymphocytes: transplantation,Dermatitis, Contact,Dermatitis, Contact: etiology,Dermatitis, Contact: immunology,Dermatitis, Contact: metabolism,Dermatitis, Contact: prevention \& control,Dermatologic Agents,Dermatologic Agents: pharmacology,Dermatologic Agents: therapeutic use,Disease Models, Animal,Endothelium, Vascular,Endothelium, Vascular: drug effects,Endothelium, Vascular: metabolism,Epoprostenol,Epoprostenol: metabolism,Integrin alpha4,Integrin alpha4: analysis,Intercellular Adhesion Molecule-1,Intercellular Adhesion Molecule-1: analysis,Male,Mice,Niacinamide,Niacinamide: analogs \& derivatives,Niacinamide: pharmacology,Niacinamide: therapeutic use,Oxazolone,Propionic Acids,Propionic Acids: pharmacology,Receptors, Prostaglandin,Receptors, Prostaglandin: drug effects,Receptors, Prostaglandin: metabolism,Skin,Skin: blood supply,Skin: drug effects,Skin: immunology,Skin: metabolism},
