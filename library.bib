journal = {Cell biology international},
keywords = {Aldosterone,Aldosterone: pharmacology,Animals,Cattle,Cell Size,Cell Size: drug effects,Cells, Cultured,Endothelium, Vascular,Endothelium, Vascular: cytology,Endothelium, Vascular: drug effects,Humans,Microscopy, Atomic Force},
