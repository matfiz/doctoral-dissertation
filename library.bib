pages = {981--9; discussion 1009--10},
pmid = {8819883},
title = {{Atomic force microscopy study of fine structures of the entire surface of red blood cells.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8819883},
volume = {9},
year = {1995}
}
