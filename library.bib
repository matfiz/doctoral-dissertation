file = {:C$\backslash$:/Users/Grzegorz Brzezinka/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rico et al. - 2005 - Probing mechanical properties of living cells by atomic force microscopy with blunted pyramidal cantilever tips.pdf:pdf},
issn = {1539-3755},
journal = {Physical Review E},
month = aug,
number = {2},
pages = {1--10},
title = {{Probing mechanical properties of living cells by atomic force microscopy with blunted pyramidal cantilever tips}},
