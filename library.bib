keywords = {Aldosterone,Aldosterone: pharmacology,Amiloride,Amiloride: pharmacology,Blotting, Western,Cell Membrane,Cell Membrane Permeability,Cell Membrane Permeability: physiology,Cell Membrane: metabolism,Cells, Cultured,Diuretics,Diuretics: pharmacology,Endothelial Cells,Endothelial Cells: cytology,Endothelial Cells: drug effects,Endothelial Cells: metabolism,Epithelial Sodium Channel,Epithelial Sodium Channel: metabolism,Fluorescent Antibody Technique,Humans,Microscopy, Atomic Force,Spironolactone,Spironolactone: pharmacology,Umbilical Veins,Umbilical Veins: cytology},
