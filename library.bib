journal = {Circulation research},
keywords = {Angiotensin II,Angiotensin II: metabolism,Angiotensin II: pharmacology,Animals,Aorta,Aorta: cytology,Cattle,Cell Respiration,Cell Respiration: drug effects,Cell Respiration: physiology,Cells, Cultured,Endothelial Cells,Endothelial Cells: cytology,Endothelial Cells: drug effects,Endothelial Cells: metabolism,Glutathione,Glutathione: metabolism,Hydrogen Peroxide,Hydrogen Peroxide: metabolism,Membrane Potential, Mitochondrial,Membrane Potential, Mitochondrial: drug effects,Mitochondria,Mitochondria: drug effects,Mitochondria: physiology,Mitochondrial Diseases,Mitochondrial Diseases: metabolism,Nitric Oxide,Nitric Oxide: metabolism,Oxidative Stress,Oxidative Stress: physiology,Superoxides,Superoxides: metabolism,Vascular Diseases,Vascular Diseases: metabolism,Vasoconstrictor Agents,Vasoconstrictor Agents: metabolism,Vasoconstrictor Agents: pharmacology},
