issn = {1524-4563},
journal = {Hypertension},
keywords = {Animals,Aorta,Aorta: metabolism,Aorta: pathology,Cells, Cultured,Disease Models, Animal,Endothelium, Vascular,Endothelium, Vascular: metabolism,Endothelium, Vascular: pathology,Epithelial Sodium Channels,Epithelial Sodium Channels: deficiency,Epithelial Sodium Channels: genetics,Epithelial Sodium Channels: metabolism,Humans,Hypertension,Hypertension: metabolism,Hypertension: pathology,Hypertension: physiopathology,Liddle Syndrome,Liddle Syndrome: metabolism,Liddle Syndrome: pathology,Liddle Syndrome: physiopathology,Mice,Mice, Inbred C57BL,Mice, Mutant Strains,Microscopy, Atomic Force,Nitric Oxide,Nitric Oxide: metabolism,RNA Interference,RNA Interference: physiology,Vascular Stiffness,Vascular Stiffness: physiology},
