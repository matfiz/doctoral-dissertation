journal = {Biophysical journal},
keywords = {Adenosine Triphosphate,Adenosine Triphosphate: metabolism,Animals,Axons,Axons: physiology,Axons: ultrastructure,Biomechanics,Biophysical Phenomena,Biophysics,Cell Membrane,Cell Membrane: drug effects,Cell Membrane: physiology,Cell Movement,Cell Movement: drug effects,Cell Movement: physiology,Cells, Cultured,Chick Embryo,Cytochalasins,Cytochalasins: pharmacology,Cytoskeleton,Cytoskeleton: drug effects,Cytoskeleton: physiology,Dimethyl Sulfoxide,Dimethyl Sulfoxide: pharmacology,Lasers,Neurons,Neurons: drug effects,Neurons: physiology,Neurons: ultrastructure,Optics and Photonics,Viscosity},
