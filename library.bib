journal = {Circulation},
keywords = {Animals,Endothelium, Vascular,Endothelium, Vascular: physiopathology,Humans,Nitric Oxide,Nitric Oxide Synthase Type III,Nitric Oxide Synthase Type III: metabolism,Nitric Oxide: biosynthesis,Oxidative Stress,Reactive Oxygen Species,Reactive Oxygen Species: metabolism,Vascular Diseases,Vascular Diseases: enzymology,Vascular Diseases: metabolism,Vascular Diseases: physiopathology},
