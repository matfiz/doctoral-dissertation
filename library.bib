journal = {Journal of cell science},
keywords = {Actin Cytoskeleton,Actin Cytoskeleton: physiology,Animals,Biomechanics,Biophysical Phenomena,Cytoskeleton,Cytoskeleton: drug effects,Cytoskeleton: metabolism,Green Fluorescent Proteins,Green Fluorescent Proteins: metabolism,Mechanotransduction, Cellular,Mechanotransduction, Cellular: physiology,Mice,Microscopy, Atomic Force,Microscopy, Confocal,Microtubules,Microtubules: metabolism,Models, Biological,Myosins,Myosins: metabolism,NIH 3T3 Cells,Nanotechnology,Photobleaching,Recombinant Fusion Proteins,Recombinant Fusion Proteins: metabolism,Stress, Mechanical},
month = feb,
number = {Pt 3},
pages = {603--13},
pmid = {22389400},
title = {{Force transduction and strain dynamics in actin stress fibres in response to nanonewton forces.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22389400},
volume = {125},
year = {2012}
}
@article{Anesti2006,
abstract = {Mitochondria are crucial organelles for life and death of the cell. They are prominent players in energy conversion and integrated signaling pathways including regulation of Ca2+ signals and apoptosis. Their functional versatility is matched by their morphological plasticity and by their high mobility, allowing their transport at specialized cellular sites. This transport occurs by interactions with a variety of cytoskeletal proteins that also have the ability to influence shape and function of the organelle. A growing body of evidence suggests that mitochondria use cytoskeletal proteins as tracks for their movement; in turn, mitochondrial morphology and function is regulated via mostly uncharacterized pathways, by the cytoskeleton.},
