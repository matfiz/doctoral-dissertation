issn = {0006-3495},
journal = {Biophysical journal},
keywords = {Animals,Biophysical Phenomena,Biophysics,Cell Line,Colloids,Cytoplasm,Cytoplasm: physiology,Elasticity,Ferric Compounds,Mice,Rheology,Rheology: methods,Viscosity},
month = jan,
number = {1 Pt 1},
pages = {573--9},
pmid = {9876170},
title = {{Measurement of local viscoelasticity and forces in living cells by magnetic tweezers.}},
