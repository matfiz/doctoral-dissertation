file = {:C$\backslash$:/Users/Grzegorz Brzezinka/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kis et al. - 2008 - Temperature-dependent elasticity of microtubules.pdf:pdf},
