file = {:C$\backslash$:/Users/Grzegorz Brzezinka/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhang, Wojcikiewicz, Moy - 2006 - Dynamic adhesion of T lymphocytes to endothelial cells revealed by atomic force microscopy.pdf:pdf},
