journal = {Journal of molecular recognition : JMR},
keywords = {Atomic Force,Blotting,Bradykinin,Bradykinin: pharmacology,Cell Line,Cell Membrane,Cell Membrane: drug effects,Cell Membrane: metabolism,Cytoskeleton,Cytoskeleton: drug effects,Cytoskeleton: metabolism,Endothelial Cells,Endothelial Cells: drug effects,Endothelial Cells: metabolism,Endothelium,HG,Hemostatics,Hemostatics: pharmacology,Humans,Microscopy,Thrombin,Thrombin: pharmacology,Umbilical Veins,Umbilical Veins: cytology,Vascular,Vascular: cytology,Vasodilator Agents,Vasodilator Agents: pharmacology,Western},
