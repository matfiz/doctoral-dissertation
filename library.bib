file = {:C$\backslash$:/Users/Grzegorz Brzezinka/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shetty et al. - 2006 - Raman spectroscopy elucidation of biochemical changes in carcinogenesis of oesophagus.pdf:pdf},
