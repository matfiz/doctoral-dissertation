journal = {Glycobiology},
keywords = {Catalase,Catalase: metabolism,Cell Adhesion,Cell Adhesion: drug effects,Cell Adhesion: physiology,Cell Membrane,Cell Membrane: drug effects,Cell Membrane: metabolism,E-Selectin,E-Selectin: metabolism,Endothelial Cells,Endothelial Cells: drug effects,Endothelial Cells: physiology,HL-60 Cells,Humans,Hymecromone,Hymecromone: analogs \& derivatives,Hymecromone: chemistry,N-Acetylneuraminic Acid,N-Acetylneuraminic Acid: chemistry,N-Acetylneuraminic Acid: metabolism,Oligosaccharides,Oligosaccharides: chemistry,Oligosaccharides: metabolism,Reactive Oxygen Species,Reactive Oxygen Species: chemistry,Reactive Oxygen Species: metabolism,Reactive Oxygen Species: pharmacology,Superoxide Dismutase,Superoxide Dismutase: metabolism,Time Factors,Xanthine Oxidase,Xanthine Oxidase: drug effects,Xanthine Oxidase: metabolism},
