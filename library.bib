keywords = {Animals,Cells, Cultured,Colloids,Colloids: chemistry,Elasticity,Fibroblasts,Fibroblasts: ultrastructure,Mice,Microscopy, Atomic Force,Silicon Dioxide,Silicon Dioxide: chemistry,Time Factors,Viscosity},
month = jan,
