keywords = {Bacterial Adhesion,Cell Membrane,Cell Membrane: metabolism,Microscopy, Atomic Force,Microscopy, Atomic Force: instrumentation,Microscopy, Atomic Force: methods,Polymers,Polymers: metabolism,Staphylococcus epidermidis,Staphylococcus epidermidis: cytology,Staphylococcus epidermidis: metabolism,Staphylococcus epidermidis: physiology},
