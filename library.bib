issn = {1530-6984},
journal = {Nano letters},
keywords = {Gold,Gold: chemistry,Microscopy, Atomic Force,Microscopy, Electron, Scanning,Nanowires,Silicon,Silicon: chemistry,inne zastosowania Raman},
mendeley-tags = {inne zastosowania Raman},
month = jan,
number = {1},
pages = {75--80},
pmid = {17212443},
title = {{The SERS and TERS effects obtained by gold droplets on top of Si nanowires.}},
