journal = {Clin. Cardiol.},
keywords = {Animals,Biological,Blood Coagulation,Blood Coagulation Factors,Blood Coagulation Factors: metabolism,Blood Coagulation: physiology,Endothelium,Epoprostenol,Epoprostenol: metabolism,Fibrinolysis,Fibrinolysis: physiology,Humans,Models,Nitric Oxide,Nitric Oxide: metabolism,Platelet Activation,Platelet Activation: physiology,Receptors,Signal Transduction,Signal Transduction: physiology,Thrombin,Thrombin: physiology,Thrombomodulin,Thrombomodulin: physiology,Vascular,Vascular: cytology,Vascular: physiology},
month = jan,
pages = {II--3 -- II--10},
